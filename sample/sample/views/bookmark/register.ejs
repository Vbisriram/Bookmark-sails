<div class="container" id="container1">
  <div class="row centered-form">

    <div class="col-xs-12 col-sm-8 col-md-4 col-sm-offset-2 col-md-offset-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title text-center">Enter Bookmark Details </h3>
        </div>
        <div class="panel-body">
          <div class="tutorialWrapper">
          <!-- <form role="form" action="create" method="POST" class="form-signin"> -->

          <form action="create" method="POST" id="sign-up-form" class="form-signin">
            <h2 class="form-signin-heading">Bookmark details </h2>
            <div class="form-group">
              <input type="text" name="title" id="title" class="form-control input-sm" placeholder="Title">
            </div>

            <div class="form-group">
              <input type="text" name="url" id="url" class="form-control input-sm" placeholder="url">
            </div>

            <div class="form-group">
              <input type="string" name="category_id" id="category_id" class="form-control input-sm" placeholder="Id">
            </div>

            <input type="submit" class="btn btn-info btn-block" value="Create Bookmark">
          </form>

    </div>
  </div>
</div>
